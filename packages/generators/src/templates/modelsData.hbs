{{#each categories}}
{{#if this.models}}
  <h3>{{this.category}}</h3>
  {{#if this.isHasSub}}
    {{#each subCategory}}
      <h4>{{this.name}}</h4>
      <table border="1">
        <thead>
           <colgroup>
            <tr>
              <th style="width: 20%;">Model ID</th>
              <th style="width: 20%;">Developer</th>
              <th style="width: 15%;">Context</th>
              <th style="width: 45%;">Model Card</th>
            </tr>
          </colgroup>
        </thead>
        <tbody>
           {{#each (lookup .. this.key)}}
            <tr>
              <td><a href="{{this.link}}.md">{{this.modelId}}</a></td>
              <td>{{this.developer}}</td>
              <td>{{this.context}}</td>
              {{#if this.modelCard}}
              <td><a href="{{this.modelCard}}" target="_blank">{{this.modalName}}</a></td>
              {{else}}
              <td>{{this.modalName}}</td>
              {{/if}}
            </tr>
          {{/each}}
        </tbody>
      </table>
    {{/each}}
  {{else}}
    <table border="1">
      <thead>
         <colgroup>
          <tr>
            <th style="width: 20%;">Model ID</th>
            <th style="width: 20%;">Developer</th>
            <th style="width: 15%;">Context</th>
            <th style="width: 45%;">Model Card</th>
          </tr>
        </colgroup>
      </thead>
      <tbody>
        {{#each this.models}}
          <tr>
            <td><a href="{{this.link}}.md">{{this.modelId}}</a></td>
            <td>{{this.developer}}</td>
            <td>{{this.context}}</td>
            {{#if this.modelCard}}
            <td><a href="{{this.modelCard}}" target="_blank">{{this.modalName}}</a></td>
            {{else}}
            <td>{{this.modalName}}</td>
            {{/if}}
          </tr>
        {{/each}}
      </tbody>
    </table>
  {{/if}}
{{/if}}
{{/each}}

