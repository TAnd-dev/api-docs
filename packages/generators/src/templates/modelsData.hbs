{{#each categories}}
{{#if this.models}}
  <h3>{{this.category}}</h3>
  {{#if this.isHasSub}}
    {{#each subCategory}}
      <h4>{{this.name}}</h4>
      <table border="1">
        <thead>
          <tr>
            <th style="width: 200px;">Model ID</th>
            <th style="width: 150px;">Developer</th>
            <th style="width: 100px;">Context</th>
            <th style="width: 300px;">Model Card</th>
          </tr>
        </thead>
        <tbody>
           {{#each (lookup .. this.key)}}
            <tr>
              <td style="width: 200px;"><a href="{{this.link}}.md">{{this.modelId}}</a></td>
              <td style="width: 150px;">{{this.developer}}</td>
              <td style="width: 100px;">{{this.context}}</td>
              {{#if this.modelCard}}
              <td style="width: 300px;"><a href="{{this.modelCard}}" target="_blank">{{this.modalName}}</a></td>
              {{else}}
              <td style="width: 300px;">{{this.modalName}}</td>
              {{/if}}
            </tr>
          {{/each}}
        </tbody>
      </table>
    {{/each}}
  {{else}}
    <table border="1">
      <thead>
        <tr>
          <th style="width: 200px;">Model ID</th>
          <th style="width: 150px;">Developer</th>
          <th style="width: 100px;">Context</th>
          <th style="width: 300px;">Model Card</th>
        </tr>
      </thead>
      <tbody>
        {{#each this.models}}
          <tr>
            <td style="width: 200px;"><a href="{{this.link}}.md">{{this.modelId}}</a></td>
            <td style="width: 150px;">{{this.developer}}</td>
            <td style="width: 100px;">{{this.context}}</td>
            {{#if this.modelCard}}
            <td style="width: 300px;"><a href="{{this.modelCard}}" target="_blank">{{this.modalName}}</a></td>
            {{else}}
            <td style="width: 300px;">{{this.modalName}}</td>
            {{/if}}
          </tr>
        {{/each}}
      </tbody>
    </table>
  {{/if}}
{{/if}}
{{/each}}

