{{!-- {{#each categories}}
{{#if this.models}}

### {{this.category}}
{{#if this.isHasSub}}
{{#each subCategory}}
#### {{this.name}}
| Model ID                | Developer     | Context | Model Card                        |
|-----------------------|-------------------|-------------------|-----------------------|
{{#each (lookup .. this.key)}}
|  [{{{this.modelId}}}]({{{this.link}}}) | {{{this.developer}}}   | {{{this.context}}}   | {{#if this.modelCard}}[{{{this.modalName}}}]({{{this.modelCard}}}){{else}}{{{this.modalName}}}{{/if}}  |
{{/each}}
{{/each}}
{{else}}

| Model ID                | Developer     | Context | Model Card                        |
|-----------------------|-------------------|-------------------|-----------------------|
{{#each this.models}}
|  [{{{this.modelId}}}]({{{this.link}}}) | {{{this.developer}}}   | {{{this.context}}}   | {{#if this.modelCard}}[{{{this.modalName}}}]({{{this.modelCard}}}){{else}}{{{this.modalName}}}{{/if}}  |
{{/each}}
{{/if}}
{{/if}}
{{/each}} --}}


{{#each categories}}
{{#if this.models}}
  <h3>{{this.category}}</h3>
  {{#if this.isHasSub}}
    {{#each subCategory}}
      <h4>{{this.name}}</h4>
      <table border="1">
        <thead>
           <colgroup>
            <tr>
              <th width="256">Model ID</th>
              <th width="140">Developer</th>
              <th width="113">Context</th>
              <th width="280">Model Card</th>
            </tr>
          </colgroup>
        </thead>
        <tbody>
           {{#each (lookup .. this.key)}}
            <tr>
              <td><a href="{{this.link}}.md">{{this.modelId}}</a></td>
              <td>{{this.developer}}</td>
              <td>{{this.context}}</td>
              {{#if this.modelCard}}
              <td><a href="{{this.modelCard}}" target="_blank">{{this.modalName}}</a></td>
              {{else}}
              <td>{{this.modalName}}</td>
              {{/if}}
            </tr>
          {{/each}}
        </tbody>
      </table>
    {{/each}}
  {{else}}
    <table border="1">
      <thead>
         <colgroup>
          <tr>
            <th width="256">Model ID</th>
            <th width="140">Developer</th>
            <th width="113">Context</th>
            <th width="280">Model Card</th>
          </tr>
        </colgroup>
      </thead>
      <tbody>
        {{#each this.models}}
          <tr>
            <td><a href="{{this.link}}.md">{{this.modelId}}</a></td>
            <td>{{this.developer}}</td>
            <td>{{this.context}}</td>
            {{#if this.modelCard}}
            <td><a href="{{this.modelCard}}" target="_blank">{{this.modalName}}</a></td>
            {{else}}
            <td>{{this.modalName}}</td>
            {{/if}}
          </tr>
        {{/each}}
      </tbody>
    </table>
  {{/if}}
{{/if}}
{{/each}}