const { OpenAI } = require('openai');

const api = new OpenAI({
  baseURL: '{{baseUrl}}',
  apiKey: '{{apiKey}}',
});

const main = async () => {
  const messages = await api.beta.threads.messages.list('<YOUR_THREAD_ID>', {
    run_id: '<YOUR_RUN_ID>',
  });

  for (const message of messages.data) {
    console.log(`Message: ${JSON.stringify(message, null, 2)}`);
  }
};

main();
