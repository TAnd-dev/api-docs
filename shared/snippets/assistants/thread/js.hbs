const { OpenAI } = require('openai');

const api = new OpenAI({
  baseURL: '{{baseUrl}}',
  apiKey: '{{apiKey}}',
});

const main = async () => {
  const thread = await api.beta.threads.create({
    messages: [
      {
        role: 'assistant',
        content: 'You are a helpful assistant.',
      }
    ],
  });

  console.log(`Thread: ${JSON.stringify(thread, null, 2)}`);
};

main();
