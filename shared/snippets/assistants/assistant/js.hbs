const { OpenAI } = require('openai');

const api = new OpenAI({
  baseURL: '{{baseUrl}}',
  apiKey: '{{apiKey}}',
});

const main = async () => {
  const assistant = await api.beta.assistants.create({
    model: '{{model}}',
    name: 'My assistant',
    instructions: 'You are a helpful assistant.',
    description: 'This is my assistant.',
  });

  console.log(`Assistant: ${assistant}`);
};

main();
