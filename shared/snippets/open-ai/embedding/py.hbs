import json
from openai import OpenAI


def main():
    client = OpenAI(
        base_url="{{baseUrl}}",
        api_key="{{apiKey}}",
    )

    text = "Your text string goes here"

    response = client.embeddings.create(input=text, model="{{model}}")
    embedding = response.data[0].embedding

    print(json.dumps(embedding, indent=2))


main()   